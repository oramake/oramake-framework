alter table flh_cached_directory
  add constraint 
    flh_cached_directory_fk_batch
  foreign key(
    existing_batch_short_name
  )
  references 
    sch_batch
  (
    batch_short_name
  )
  on delete set null
/
alter table flh_cached_directory
  add constraint 
    flh_cached_directory_fk_oper
  foreign key(
    operator_id
  )
  references 
    op_operator
  (
    operator_id
  )
/    
