-- table: flh_request_state
-- Состояния запросов к модулю FileHandler
--
create table flh_request_state(
  request_state_code			varchar2( 10 ) not null,
  date_ins					date default sysdate not null,
  operator_id				integer not null,
  constraint flh_request_state_pk primary key(request_state_code)
    using index tablespace &indexTablespace
)
/
comment on table flh_request_state is
'Состояния запросов к модулю FileHandler
[ SVN root: Oracle/Module/FileHandler]
'
/
