--table: tmp_file_name
--
--Временная таблица для возврата данных об элементах каталога.
--
create global temporary table tmp_file_name(
  file_name_id                integer                         not null
  , file_name                 varchar2(255)                   not null
  , file_size                 integer                         not null
  , last_modification         timestamp with local time zone  not null
  , constraint tmp_file_name_pk primary key ( file_name_id)
)
/
