<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<web-app>
  <servlet>
    <servlet-name>remoteLoggerServiceImpl</servlet-name>
    <servlet-class>com.allen_sauer.gwt.log.server.RemoteLoggerServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>remoteLoggerServiceImpl</servlet-name>
    <url-pattern>/Scheduler/gwt-log</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MainServiceServlet</servlet-name>
    <servlet-class>com.technology.jep.jepria.server.service.JepMainServiceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServiceServlet</servlet-name>
    <url-pattern>/Scheduler/MainService</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BatchServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.batch.server.BatchServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BatchServlet</servlet-name>
    <url-pattern>/Scheduler/BatchService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/BatchBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.batch.server.ejb.BatchLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>ScheduleServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.schedule.server.ScheduleServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScheduleServlet</servlet-name>
    <url-pattern>/Scheduler/ScheduleService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/ScheduleBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.schedule.server.ejb.ScheduleLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>IntervalServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.interval.server.IntervalServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IntervalServlet</servlet-name>
    <url-pattern>/Scheduler/IntervalService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/IntervalBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.interval.server.ejb.IntervalLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>RootLogServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.rootlog.server.RootLogServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RootLogServlet</servlet-name>
    <url-pattern>/Scheduler/RootLogService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/RootLogBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.rootlog.server.ejb.RootLogLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>DetailedLogServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.detailedlog.server.DetailedLogServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DetailedLogServlet</servlet-name>
    <url-pattern>/Scheduler/DetailedLogService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/DetailedLogBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.detailedlog.server.ejb.DetailedLogLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>BatchRoleServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.batchrole.server.BatchRoleServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BatchRoleServlet</servlet-name>
    <url-pattern>/Scheduler/BatchRoleService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/BatchRoleBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.batchrole.server.ejb.BatchRoleLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>OptionServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.option.server.OptionServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OptionServlet</servlet-name>
    <url-pattern>/Scheduler/OptionService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/OptionBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.option.server.ejb.OptionLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>ValueServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.value.server.ValueServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValueServlet</servlet-name>
    <url-pattern>/Scheduler/ValueService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/ValueBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.value.server.ejb.ValueLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>ModuleRolePrivilegeServlet</servlet-name>
    <servlet-class>com.technology.oracle.scheduler.moduleroleprivilege.server.ModuleRolePrivilegeServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ModuleRolePrivilegeServlet</servlet-name>
    <url-pattern>/Scheduler/ModuleRolePrivilegeService</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/ModuleRolePrivilegeBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.oracle.scheduler.moduleroleprivilege.server.ejb.ModuleRolePrivilegeLocal</local>
  </ejb-local-ref>
  <servlet>
    <servlet-name>VersionServlet</servlet-name>
    <servlet-class>com.technology.jep.jepria.server.version.VersionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VersionServlet</servlet-name>
    <url-pattern>/versionServlet</url-pattern>
  </servlet-mapping>
  <ejb-local-ref>
    <ejb-ref-name>ejb/VersionBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <local>com.technology.jep.jepria.server.version.ejb.JepVersionLocal</local>
  </ejb-local-ref>
  <welcome-file-list>
    <welcome-file>Scheduler.jsp</welcome-file>
  </welcome-file-list>
  <security-role>
    <role-name>SchShowBatch</role-name>
  </security-role>
  <security-role>
    <role-name>SchShowSchedule</role-name>
  </security-role>
  <security-role>
    <role-name>SchShowLog</role-name>
  </security-role>
  <security-role>
    <role-name>SchShowBatchRole</role-name>
  </security-role>
  <security-role>
    <role-name>SchShowBatchOption</role-name>
  </security-role>
  <security-role>
    <role-name>SchShowModuleRolePrivilege</role-name>
  </security-role>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>The application entry point</web-resource-name>
      <url-pattern>/*</url-pattern>
      <http-method>*</http-method>
    </web-resource-collection>
    <auth-constraint>
      <role-name>SchShowBatch</role-name>
      <role-name>SchShowSchedule</role-name>
      <role-name>SchShowLog</role-name>
      <role-name>SchShowBatchRole</role-name>
      <role-name>SchShowBatchOption</role-name>
      <role-name>SchShowModuleRolePrivilege</role-name>
    </auth-constraint>
  </security-constraint>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>Version Servlet entry point</web-resource-name>
      <url-pattern>/versionServlet</url-pattern>
      <http-method>GET</http-method>
      <http-method>POST</http-method>
    </web-resource-collection>
  </security-constraint>
</web-app>
